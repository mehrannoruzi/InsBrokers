@model PagingListDetails<User>
@{
    ViewBag.Title = $"{Strings.Management} {InsBrokers.Domain.Resource.Strings.User}";
    Layout = "_LayoutManage";
}

@section styles{
    <style>
        @@media(min-width: 700px) {
            .modal-dialog {
                min-width: 700px !important;
            }
        }
    </style>
}

@section filter{

    <partial name="Partials/_Filters" model="new UserSearchFilter()" />
}
<partial name="Partials/_List" model="@Model" />
