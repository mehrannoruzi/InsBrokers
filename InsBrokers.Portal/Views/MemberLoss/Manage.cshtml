@model PagingListDetails<Loss>
@{
    ViewBag.Title = $"{Strings.Management} {InsBrokers.Domain.Resource.Strings.Loss}";
    Layout = "_PortalLayoutManage";
}
@section styles{
    <link href="@Url.Content("~/Styles/App/LossAsset/uploader.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Styles/App/customize-template.min.css")" rel="stylesheet" />
}
@section filter{
    <partial name="Partials/_Filters" model="new LossSearchFilter { LossType = null }" />
}
<partial name="Partials/_List" model="@Model" />
@section scripts{
    <script src="@Url.Content("~/Scripts/App/LossAsset/uploader.js")" asp-append-version="true"></script>
    <script src="@Url.Content("~/Scripts/App/MemberLoss/manage.js")" asp-append-version="true"></script>
}