@model BankAccountSearchFilter
@{
    Layout = null;
}

<div class="row">
    <div class="col-12 col-sm-6">
        <custom-select-for for="Name" optional-label="@Strings.PleaseSelect" class="form-control" items="EnumExtension.GetEnumElements<BankName>().Select(x=>new SelectListItem {
                Text = x.Description,
                Value = x.Name
            }).ToList()" />
    </div>
</div>
